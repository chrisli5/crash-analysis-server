{
    "parserOptions": {
        "es6": true,
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
          "jsx": true
        },
        "requireConfigFile": false
      },
      "parser": "@babel/eslint-parser",
      "rules": {
        "no-unused-vars": 0
      },
      "env": {
        "browser": true,
        "node": true
      },
    "extends": ["eslint:recommended", "airbnb-base", "prettier"]
}
